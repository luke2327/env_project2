    <script type="text/javascript">
    var APP_URL = "https:\/\/www.hearthstudy.com";
    $.ajaxSetup({
        headers: {
            'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
        }
    });

    $.material.init();
    
    $(document).ready(function(){
        $(".navbar-fixed-top").autoHidingNavbar({
            // see next for specifications
        });
        //$('[data-toggle="tooltip"]').tooltip();
        $('a[data-toggle="tooltip"]').tooltip({
           container: 'body'
        });

        $('[data-toggle="popover"]').popover({html: "true"});

    });


    $(window).scroll(function(){
        var windowpos = $(window).scrollTop();
        var martop = 70;
        if ($('#fixedpin1').length){
            var fixedpintop = $('#fixedpin1').offset().top;
            var fixedpinbottom = fixedpintop + $('#fixedpin1').height();
            var rad = $('#pin1');

            if (windowpos + martop >= fixedpinbottom) {
                rad.css({
                    position: 'fixed',
                    top: martop
                });
            } else {
                rad.css({
                    position: 'relative',
                    top: 0
                });
            }
        }

        if ($('#fixedpin2').length){
            var fixedpintop = $('#fixedpin2').offset().top;
            var fixedpinbottom = fixedpintop + $('#fixedpin2').height();
            var rad = $('#pin2');

            if (windowpos + martop >= fixedpinbottom) {
                rad.css({
                    position: 'fixed',
                    top: martop
                });
            } else {
                rad.css({
                    position: 'relative',
                    top: 0
                });
            }
        }

        if ($(this).scrollTop() > 1024) {
            //$('.scrolltotop').fadeIn();
            $('.scrolltotop').show();
            $('.movetodeck').show();
        } else {
            //if ( $(this).scrollTop() == 0 ) { changeMenuImg(); }
            //$('.scrolltotop').fadeOut();
            $('.scrolltotop').hide();
            $('.movetodeck').hide();
        }

        if ($(this).scrollTop() > 10) {
            $('#top-nav').addClass('fixed-header');
        } else {
            $('#top-nav').removeClass('fixed-header');
        }
    });

    // Click event to scroll to top
    $('.scrolltotop').click(function(){
        $('html, body').animate({scrollTop : 0}, 1500, 'easeInOutExpo');
        return false;
    });

    $('.scrollto').click(function(){
        var pos = $(this).attr("pos");
        var goTo = $('#'+pos).offset().top;
        $('html, body').animate({scrollTop : goTo}, 500, 'easeInOutExpo');
        return false;
    });

    </script>
    
    <script type="text/javascript" src="https://cdnjs.cloudflare.com/ajax/libs/OwlCarousel2/2.2.1/owl.carousel.min.js"></script>

    <script type='text/javascript' src="https://cdnjs.cloudflare.com/ajax/libs/jquery-validate/1.16.0/jquery.validate.min.js"></script>

    <script type='text/javascript' src="https://www.hearthstudy.com/js/jquery.rateit.js?ver=1.1.1"></script>
    <script type="text/javascript" src="https://www.hearthstudy.com/js/hs.js?ver=1.1.5"></script>
    <script type="text/javascript" src="https://www.hearthstudy.com/js/hs.app.js?ver=1.1.2"></script>

    <script type="text/javascript">
$(document).ready(function () {
    var cardratearray = [];
    $('.carvote .rateit').each(function () {
        cardratearray.push($(this).data('productid'));
    });

    $.ajax({
        type: 'POST',
        url: APP_URL+'/cardsrateview',
        data: {
            carray: cardratearray
        },
        dataType: 'json',
        success: function(list) {
            $.each(list, function () {
                var ri = $('.carvote .rateit[data-productid="'+this.cardid+'"]');

                ri.rateit();
                ri.rateit('value',this.rate);
                ri.attr('data-vote-cnt',this.votecnt);
                //ri.attr('data-org-value',this.value);
                //ri.attr('data-rateit-value',this.value);
                ri.parent().find('.voteinfo').find('span.current-rating')
                    .find('span.votecnt')
                    .text(Number(this.votecnt));

                ri.parent().find('.voteinfo').find('span.current-rating')
                    .find('span.value')
                    .text(this.rate);

            });
        },
        fail: function (result) {
            alert('Card Vote Error =>');
        }
    });

    $('.carvote .rateit').bind('rated', function (e) {
        e.preventDefault();
        e.stopPropagation();
        
        var ri = $(this);
        var productID = ri.data('productid'); 
        var votecnt = ri.data('vote-cnt');
        var orgvalue = Number(ri.data('org-value'));
        var value = Number(ri.rateit('value'));
        var temp = '';

        $.ajax({
            url: "https://www.hearthstudy.com/cardvote",
            type: 'POST',
            data: {
                ownid: productID,
                value: value,
            },
            dataType: 'json',
            success: function (result) {
                if ( result != 'rate' ) {
                    ri.rateit('value',orgvalue);
                    ri.rateit('readonly', true);
                    //ri.attr('data-content') = "이미 투표를 완료한 카드입니다."
                    //alert("이미 투표를 완료한 카드입니다.");

                    temp = ri.attr('data-content');

                    ri.attr('data-content','이미 투표를 완료한 카드입니다.');

                    setTimeout(function(){
                        ri.popover('show');
                    }, 200);
                    setTimeout(function(){
                        ri.popover('hide');
                    }, 2000);
                    setTimeout(function(){
                        ri.attr('data-content', temp);
                    }, 200);
                }
                else {
                    var resultrate = ((orgvalue*votecnt)+value)/(Number(votecnt)+1);

                    //alert(value+"점 부여하였습니다. 현재 평점은 "+resultrate.toFixed(2)+"점 입니다. 감사합니다.");

                    ri.rateit('readonly', true);
                    ri.rateit('value',resultrate.toFixed(2));

                    ri.parent().find('.voteinfo').find('span.current-rating')
                        .find('span.votecnt')
                        .text(Number(votecnt)+1);

                    ri.parent().find('.voteinfo').find('span.current-rating')
                        .find('span.value')
                        .text(resultrate.toFixed(2));

                    //ri.attr('data-content') = "이미 투표를 완료한 카드입니다."
                    //alert("이미 투표를 완료한 카드입니다.");
                    ri.attr('data-content',value+"점 부여하였습니다. 현재 평점은 "+resultrate.toFixed(2)+"점, 투표수는 "+(Number(votecnt)+1)+"입니다.");

                    setTimeout(function(){
                        ri.popover('show');
                    }, 200);
                    setTimeout(function(){
                        ri.popover('hide');
                    }, 2000);

                }
            },
            fail: function (result) {
                alert('Card Vote Error =>');
            }
        });
    });
});

$(document).on("click", ".clipboard", function(event) {
    event.preventDefault();
    event.stopPropagation();
    var target = $(this);
    var ran = Math.floor((Math.random() * 100) + 1);    
    $('<span class="clipboard-copy no'+ran+' notyet btn btn-xs btn-raised btn-danger">복사</span>').appendTo(target);
    target.find('.clipboard-copy.no'+ran).removeClass('notyet');
    setTimeout(function(){
        target.find('.clipboard-copy.no'+ran).remove();
    }, 2500);
});
    </script>
