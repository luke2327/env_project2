$('#autocardsearch').submit(function(event) {
    /* Stop form from submitting normally */
    event.preventDefault();

    /* Get from elements values */
    var values = $('#autocompletecardname').val();

    //showloader();
    /* for ie */
    var form = document.createElement('form');
    var objs;
    objs = document.createElement('input');
    objs.setAttribute('type', 'text');
    objs.setAttribute('name', 'name');
    objs.setAttribute('value', values);
    form.appendChild(objs);
    form.setAttribute('method', 'GET');
    form.setAttribute('action', APP_URL+"/card/");
    document.body.appendChild(form);
    form.submit();
});

$(function() {
    $("#autocompletecardname").autocomplete({
        source: function(request, response) {
            $.ajax({
                url: "https://www.hearthstudy.com/autocomplete",
                dataType: "json",
                data: {
                    searchValue: request.term
                },
                success: function(json) {
                    response($.map(json, function(item) {
                        //return{
                        //cardname:item.name,
                        //xvalue:request.term,
                        //cardid:item.id,
                        //cardset:item.category
                        //};
                        return {
                            label: item.name,
                            value: item.name,
                            id: item.id,
                            set: item.category,
                            rarity: item.rarity,
                            collect: item.collectible,
                            enus: item.en_name
                        }
                    }));
                }
            });
        },
        focus: function(event, ui) {
            return false;
            //event.preventDefault(); // Prevent the default focus behavior.
            //this.value = ui.item.label;
        },
        minLength: 1,
        select: function(event, ui) {
            var url = "https://www.hearthstudy.com/card/"+ui.item.id;
            window.open(url, '_self');
        }
    }).data("ui-autocomplete")._renderItem = function(ul, item) {
        var coltext = '덱 구성 불가 카드';
        if (item.collect) coltext = '덱 구성 가능 카드';
        return $("<li class=rarity_" + item.rarity + ">" + item.label + " (" + item.enus + ")</li>").append('<img class="lazy-loaded" data-src="'+APP_URL+'/images/' + item.set + '_Icon_brown.png" width="20" />').append('<small class=rarity_FREE>' + ' ' + coltext + '</small>').appendTo(ul);
    };
});


$(window).load(function() {
    $('.masonrycont').masonry({
        itemSelector: '.msry',
        columnWidth: '.msry'
    });


    $('.card-element').plate({
        inverse: false,
        perspective: 600,
        maxRotation: 20
    });
});

</script>

<script>

        $(window).scroll(function() {
            var windowpos = $(window).scrollTop();
            var fpos = $('#hstop').offset();
            var lad = $('.leftad');
            var lad2 = $('.rightad');

            if (window.innerWidth >= 1600) {
                if (windowpos >= fpos.top) {
                    lad.css({
                        position: 'fixed',
                        top: 0
                    });
                    lad2.css({
                        position: 'fixed',
                        top: 0
                    });
                } else {
                    lad.css({
                        position: 'absolute',
                        top: fpos.top
                    });
                    lad2.css({
                        position: 'absolute',
                        top: fpos.top
                    });
                }
            }

        });

</script>
